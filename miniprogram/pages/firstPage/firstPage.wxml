<swiper class="card-swiper round-dot" indicator-dots="true" circular="true" autoplay="true" interval="3000" duration="700" bindchange="cardSwiper" indicator-color="#8799a3" indicator-active-color="#0081ff">
  <swiper-item wx:for="{{swiperList}}" wx:key class="{{cardCur==index?'cur':''}}">
    <view class="swiper-item">
      <image src="{{item.url}}" mode="aspectFill"></image>
    </view>
  </swiper-item>
</swiper>

